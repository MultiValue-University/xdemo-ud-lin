OPEN 'VOC' TO F.VOC ELSE STOP "NO VOC FILE"
READ A.VOC FROM F.VOC,'LIST' ELSE STOP "NO READ"
WRITE A.VOC ON F.VOC,'TESTPROG-MAIN' ON ERROR STOP "NO WRITE"
A='' ; B=''
CALL LOCALSUB(A,B) ;*local sub
CALL TESTSUB(A,B) ;*globally cataloged
CALL TESTSUB1(A,B) ;*locally cataloged
WRITE A.VOC ON F.VOC,'TESTPROG-AFT-CALLS' ON ERROR STOP "NO WRITE"
STOP
SUBROUTINE LOCALSUB(A,B)
OPEN 'VOC' TO F.VOC ELSE STOP "NO VOC FILE"
READ A.VOC FROM F.VOC,'LIST' ELSE STOP "NO READ"
WRITE A.VOC ON F.VOC,'LOCALSUB-TESTPROG' ON ERROR STOP "NO WRITE"
RETURN
END